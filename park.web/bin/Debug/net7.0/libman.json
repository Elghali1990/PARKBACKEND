{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.7.0",
      "destination": "wwwroot/jquery/",
      "files": [
        "jquery.min.js"
      ]
    },
    {
      "library": "bootstrap@5.3.0",
      "destination": "wwwroot/bootstrap/",
      "files": [
        "js/bootstrap.min.js",
        "css/bootstrap.css"
      ]
    },
    {
      "library": "bootstrap-icons@1.10.5",
      "destination": "wwwroot/bootstrap-icons/",
      "files": [
        "bootstrap-icons.svg",
        "font/fonts/bootstrap-icons.woff",
        "font/fonts/bootstrap-icons.woff2",
        "font/bootstrap-icons.css",
        "font/bootstrap-icons.json",
        "font/bootstrap-icons.min.css"
      ]
    },
    {
      "library": "popper.js@2.11.8",
      "destination": "wwwroot/popper.js/",
      "files": [
        "umd/popper.min.js"
      ]
    }
  ]
}